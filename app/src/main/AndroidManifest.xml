<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="tech.gonzalocaballero.animacionestest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".AttensionActivity">
        </activity>
        <activity android:name=".SpecialActivity">
        </activity>
        <activity android:name=".BounceActivity">
        </activity>
        <activity android:name=".FadeActivity">
        </activity>
        <activity android:name=".FlipActivity">
        </activity>
        <activity android:name=".RotateActivity">
        </activity>
        <activity android:name=".SlideActivity">
        </activity>
        <activity android:name=".ZoomActivity">
        </activity>
    </application>

</manifest>